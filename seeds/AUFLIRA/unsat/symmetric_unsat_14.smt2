(declare-fun a13 () (Array Int (Array Int Real)))
(declare-fun e13 () Real)
(assert (forall ((?a (Array Int (Array Int Real))) (?n Int)) (= (symmetric ?a ?n) (forall ((?i Int) (?j Int)) (=> (and (<= 1 ?i) (<= ?i ?n) (<= 1 ?j) (<= ?j ?n)) (= (select (select ?a ?i) ?j) (select (select ?a ?j) ?i)))))))
(assert (symmetric a0 n))
(assert (= a1 (store a0 0 (store (select a0 0) 0 e0))))
(assert (= a2 (store a1 1 (store (select a1 1) 1 e1))))
(assert (= a3 (store a2 2 (store (select a2 2) 2 e2))))
(assert (= a4 (store a3 3 (store (select a3 3) 3 e3))))
(assert (= a5 (store a4 4 (store (select a4 4) 4 e4))))
(assert (= a6 (store a5 5 (store (select a5 5) 5 e5))))
(assert (= a7 (store a6 6 (store (select a6 6) 6 e6))))
(assert (= a8 (store a7 7 (store (select a7 7) 7 e7))))
(assert (= a9 (store a8 8 (store (select a8 8) 8 e8))))
(assert (= a10 (store a9 9 (store (select a9 9) 9 e9))))
(assert (= a11 (store a10 10 (store (select a10 10) 10 e10))))
(assert (= a12 (store a11 11 (store (select a11 11) 11 e11))))
(assert (= a13 (store a12 12 (store (select a12 12) 12 e12))))
(assert (not (symmetric a13 n)))
(check-sat)
(exit)
