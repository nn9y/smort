(set-info :smt-lib-version 2.6)
;;; Processed by pysmt to remove constant-real bitvector literals
(set-logic QF_BVFP)
(set-info :source |SPARK inspired floating point problems by Florian Schanda|)
(set-info :category "crafted")
(set-info :status unsat)
(define-fun isFinite ((f Float32)) Bool (or (fp.isNormal f) (fp.isSubnormal f) (fp.isZero f)))
(define-fun f0_25 () Float32 ((_ to_fp 8 24) #x3e800000))
(define-fun f1 () Float32 ((_ to_fp 8 24) #x3f800000))
(define-fun s_max () Float32 ((_ to_fp 8 24) #x41200000))
(define-fun s_msb () Float32 ((_ to_fp 8 24) #x41a00000))
(define-fun s_scale () Float32 ((_ to_fp 8 24) #b01000101010011001100110011001101))
(declare-fun x () (_ BitVec 16))
(declare-fun y () Float32)
(assert (and (fp.geq y f0_25) (fp.leq y f1)))
(define-fun res_1 () Float32 (fp.div RNE ((_ to_fp_unsigned 8 24) RNE ((_ zero_extend 16) x)) s_scale))
(assert (not (isFinite res_1)))
(check-sat)
(exit)
