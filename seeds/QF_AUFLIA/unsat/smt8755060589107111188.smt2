(declare-sort Color 0)
(declare-fun a () Int)
(declare-fun b () Int)
(declare-fun il_pass () Int)
(declare-fun il_tl () Color)
(declare-fun ml_pass () Int)
(declare-fun red () Color)

(assert (! (= il_tl red)
         :named hyp1))
(assert (! (< 0 b)
         :named hyp2))
(assert (! (= a 0)
         :named hyp3))
(assert (! (= ml_pass 1)
         :named hyp4))
(assert (! (= il_pass 1)
         :named hyp5))
(assert (! (or 
               (< 1 b) 
               (= b 1))
         :named hyp6))
(assert (! (not 
               (< 1 (+ 1 1)))
         :named goal))
(check-sat)
(exit)

