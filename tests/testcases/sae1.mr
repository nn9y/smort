(seed s sat)
(morph m sat)

(notation x s)
(notation y s)
(notation a :cons)

(subst-template 
    ((x Int) (y Int) (a Int))
    ( ((< x y) (<= x y)) ((< x y) (distinct x y)) ((<= x y) (< x (+ y a))) )
)
(subst-template 
    ((x Real) (y Real) (a Real))
    ( ((< x y) (<= x y)) ((< x y) (distinct x y)) ((<= x y) (< x (+ y a))) ) 
)
(subst-template
    ((x String) (y String) (a String))
    ( ((str.< x y) (str.<= x y)) ((str.<= x y) (str.<= x (str.++ y a))) ) 
)

(method "LPI" c :seed)

(fuse c)