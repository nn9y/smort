(seed s1 sat)
(seed s2 sat)
(mutant m sat)

(notation x s1)
(notation y s2)
(notation z :var)
(notation c :cons)

(subst-template :free ((x Int) (y Int) (z Int)) ((x (div z y)) (y (div z x)) (z (* x y))) )
(subst-template :free ((x Int) (y Int) (z Int) (c Int)) ((x (- (- z y) c)) (y (- (- z x) c)) (z (+ (+ x y) c))) )

(fuse (and s1 s2))